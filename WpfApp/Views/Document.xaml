<UserControl x:Class="WpfApp.Views.Document"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="AliceBlue">
        
        <StackPanel>
            <TextBlock Text="文档界面" Height="50" Width="200" FontSize="40" Margin="5,10,5,5" HorizontalAlignment="Center" TextAlignment="Center" />
            <TextBox x:Name="DisplayAssemblyInfo" Height="200" Background="LightGray" Margin="5" />
            <Button x:Name="GetAssembly" Click="GetAssembly_Click" Content="GetAssembly" Margin="5" />
        </StackPanel>
    </Grid>
</UserControl>
